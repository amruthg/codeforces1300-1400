#include<bits/stdc++.h>
using namespace std;

int main(){

	int n;
	cin>>n;

	int h1=0,h2=0,input=0;
	for(int i=0;i<n;i++){

		cin>>input;
		if(input==100){
			h1+=1;
			
		}

		else{
			h2+=1;
			
		}
	}

	if(h1%2!=0){

		cout<<"NO";
		return 0;
	}

	if(h1/2>=1 && h2%2!=0){

		cout<<"YES";
		return 0;
	}

	if(h1%2==0 && h2%2==0){

		cout<<"YES";
		return 0;
	}

}
